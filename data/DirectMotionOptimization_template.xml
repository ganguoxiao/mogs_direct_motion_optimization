<?xml version="1.0" encoding="UTF-8"?>
<problem type = "DirectMotionOptimization">
    <robot>
        <!--You can add how many robots you want, by their name if they are register in the project or their URL and with their contact properties file-->
        <name>MyRobot</name>
    </robot>
    <environment>
        <!--You can add environment files (for the moment it will be used only for the visualization but futures works will used it for collisions-->
        <name>My environment</name>
    </environment>
    <solver>
        <!--For the moment we can use only the IPOPT solver.-->
        <name>IPOPT</name>
<!--         <options option="option name" type="interger,double,string">options value (see the ipopt documentation)</options> -->
    </solver>
    <parameters>
	    <constraint_on_q>yes/no</constraint_on_q>
	    <constraint_on_dq>yes/no</constraint_on_dq>
	    <coeff_dq_max>1.0</coeff_dq_max>
	    <optim_motion_duration>no</optim_motion_duration>	<!--  for the moment no-->    
	    <constraint_on_torques>no</constraint_on_torques> <!--  for the moment no-->    
	    <criteria>none/energyjerk/fitting</criteria>	       
	    <parallelization>no_thread/multi_thread</parallelization>
	    <integration_step> 0.001	</integration_step>
    </parameters>
     <motion>
	  <init_posture>	</init_posture>
	  <final_posture>	</final_posture>
	  <motion_duration>1.0	</motion_duration>
	  
     </motion>
     <result> </result>
</problem>
